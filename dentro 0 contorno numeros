#include <iostream>
using namespace std;
int main() {
    int n=5,fin=1;
    int arr[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if (i == 0 || i == 4 || j == 0 || j == 4) {
            arr[i][j] = fin;
            fin++;
            } else { 
            arr[i][j] = 0;
            }
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
