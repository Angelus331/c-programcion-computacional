#include <iostream>
using namespace std;
int main() {
    int arr[3][3]={2,3,4,5,6,7,8,9,1};
    cout<<endl;
    int temp =arr[0][0];
    arr[0][0]=arr[2][2];
    arr[2][2]=temp;
    int temp2=arr[0][2];
    arr[0][2]=arr[2][0];
    arr[2][0]=temp2;
    for (int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            cout<<arr[i][j]<<" ";
    }
        cout<<endl;
    }
    return 0;
}
