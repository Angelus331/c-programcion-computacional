#include <iostream>
using namespace std;
int main() {
    int arr[3][3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            for(int k=0;k<3;k++){
             if ((i == 0 || i == 2) && (j == 0 || j == 2) && (k == 0 || k == 2)) {
                    arr[i][j][k] = 0;
                } else {
                    arr[i][j][k] = 1;
            }
                cout<<arr[i][j][k]<<" ";
            }
              cout<<endl;
        }
        cout<<endl;
    }
    return 0;
}
