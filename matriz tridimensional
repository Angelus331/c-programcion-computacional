#include <iostream>
using namespace std;
int main() {
    int arr[6][3][3];
    int fin=1;
    for(int i=0;i<6;i++)
    {
        for(int j=0;j<3;j++)
        {
            for(int k=0;k<3;k++){
                arr[i][j][k]= fin;
                fin++;
                cout<<arr[i][j][k]<<" ";
                
            }
              cout<<endl;
        }
          cout<<endl;
    }
    return 0;
}
